<Page x:Class="up_wpf.Pages.EngPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:up_wpf.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EngPage">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Margin="5" Padding="5" Click="Button_Click">Учителя</Button>
            <Button Margin="5" Padding="5" Click="Button_Click_3">Дисциплины</Button>
            <Button Margin="5" Padding="5" Click="Button_Click_2">Кафедры</Button>
            <Button Margin="5" Padding="5" Click="Button_Click_1">Экзамены</Button>
            <Button Margin="5" Padding="5" Click="Button_Click_4">Заведующие кафедрой</Button>
        </StackPanel>
    </Grid>
</Page>
